<%- include('../partials/header') %>
<link rel = 'stylesheet' href="/stylesheets/style.css" />
<table id="list">
  <thead>
    <tr>
      <th>Goals</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% goals.forEach(function(g) { %>
      <tr>
        <td><%= g.title %></td>
        <td><a href="/notes/index<%= g._id %>">DETAILS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>